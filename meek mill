<style>
  /* This is the magic class */
.prominent {
  /*
  Because the browser will jump to the exact offset-top of the element
  I added some space at the top to make it look more "centered".
  */
  @space: 50px;
  
  @-webkit-keyframes fadeIn {
      from { opacity: .5; }
        to { opacity: 2.0; }
  }

  @keyframes fadeIn {
      from { opacity: .5; }
        to { opacity: 2.0; }
  }

  .prominent-close {
    display: none;
  }

  &:target {
    position: relative;
    z-index: 1000!important;
    padding-top: @space;
    margin-bottom: -@space;
    top: -@space;
    
    .prominent-close {
      display: block;
      position: fixed;
      text-indent: -100px;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      z-index: -1;
    }

    &:before {
      display: block;
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: #000;
      content: '';
      z-index: -1;
      opacity: .8;
      -webkit-animation: fadeIn .9s;
      animation: fadeIn .9s
    }
  }
}</style>
